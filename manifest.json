{
  "manifest_version": 2,
  "name": "Chaos Laptop Club",
  "description": "Laptops are the future!",
  "version": "1.2",
  "icons": {
    "48": "icons/logo_48px.png",
    "96": "icons/logo_96px.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "assets/*.png",
    "assets/*.svg"
  ]
}
